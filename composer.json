{
    "name": "dibiy/mginstallable",
    "description": "installable package",
    "authors": [
        {
            "name": "Yassine Dibi",
            "email": "dibiyassine@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "mgInstallable\\installable\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "mgInstallable\\installable\\Providers\\MgInstallableServiceProvider"
            ]
        }
    }
}
